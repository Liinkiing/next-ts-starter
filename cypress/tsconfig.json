{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es6"],
    "strict": true,
    "types": ["cypress", "node"],
    "typeRoots": ["@types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~e2e/*": ["./*"],
      "~/*": ["../src/*"],
      "~pages/*": ["./pages/*"]
    }
  }
}
