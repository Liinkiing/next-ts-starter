  },
  "author": "Omar Jbara <omar.jbara2@gmail.com>",
  "dependencies": {
    "@styled-system/should-forward-prop": "^5.1.5",
    "dotenv-load": "^2.0.0",
    "framer-motion": "^2.0.0-beta.42",
    "next": "^9.3.4",
    "next-compose-plugins": "^2.2.0",
    "next-env": "^1.1.1",
    "next-fonts": "^1.0.3",
    "next-images": "^1.4.0",
    "nprogress": "^0.2.0",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/nprogress": "^0.2.0",
